<SubscriptionScreen>:
    BoxLayout:
        orientation: 'vertical'
        padding: 20
        spacing: 10

        Label:
            text: "Subscription Details"
            font_size: 24

        Button:
            text: "Back to Dashboard"
            on_press: 
                root.manager.current = 'user_dashboard' if root.manager.current == 'subscription' else 'admin_dashboard'
